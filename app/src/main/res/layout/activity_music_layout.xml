<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MusicActivity">

    <ImageButton
        android:id="@+id/musicB1"
        android:layout_width="155dp"
        android:layout_height="180dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/musicB2"
        app:srcCompat="@drawable/ic_eigth_note_double"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteX="-12dp"
        android:background="#00E0F9FF"
        android:padding="20dp"/>

    <ImageButton
        android:id="@+id/musicB2"
        android:layout_width="95dp"
        android:layout_height="180dp"
        android:layout_marginStart="152dp"
        android:layout_marginLeft="152dp"
        android:visibility="visible"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@id/musicB3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_half_note"
        tools:visibility="visible"
        android:background="#00E0F9FF"
        android:padding="20dp"/>

    <ImageButton
        android:id="@+id/musicB3"
        android:layout_width="135dp"
        android:layout_height="180dp"
        android:background="#00E0F9FF"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/musicB3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_single_sixteenth_note"
        android:padding="20dp"/>

    <ImageButton
        android:id="@+id/musicB4"
        android:layout_width="115dp"
        android:layout_height="180dp"
        android:layout_marginTop="40dp"
        android:background="#00E0F9FF"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/musicB1"
        app:srcCompat="@drawable/ic_eigth_drive" />

    <ImageButton
        android:id="@+id/musicB5"
        android:layout_width="130dp"
        android:layout_height="180dp"
        android:layout_marginTop="40dp"
        android:background="#00E0F9FF"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@id/musicB4"
        app:layout_constraintTop_toBottomOf="@id/musicB2"
        app:srcCompat="@drawable/ic_treble_clef" />

    <ImageButton
        android:id="@+id/musicB6"
        android:layout_width="115dp"
        android:layout_height="180dp"
        android:layout_marginTop="40dp"
        android:background="#00E0F9FF"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@id/musicB5"
        app:layout_constraintTop_toBottomOf="@id/musicB3"
        app:srcCompat="@drawable/ic_dotted_quarter_note" />

    <ImageButton
        android:id="@+id/musicB7"
        android:layout_width="95dp"
        android:layout_height="180dp"
        android:layout_marginTop="44dp"
        android:background="#00E0F9FF"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintTop_toBottomOf="@id/musicB4"
        app:srcCompat="@drawable/ic_quarter_note"
        tools:layout_editor_absoluteX="0dp" />

    <ImageButton
        android:id="@+id/musicB8"
        android:layout_width="115dp"
        android:layout_height="180dp"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="44dp"
        android:background="#00E0F9FF"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/musicB5"
        app:srcCompat="@drawable/ic_dotted_half_note" />

    <ImageButton
        android:id="@+id/musicB9"
        android:layout_width="135dp"
        android:layout_height="180dp"
        android:layout_marginTop="44dp"
        android:background="#00E0F9FF"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@id/musicB8"
        app:layout_constraintTop_toBottomOf="@id/musicB6"
        app:srcCompat="@drawable/ic_quarter_note_double" />

</androidx.constraintlayout.widget.ConstraintLayout>